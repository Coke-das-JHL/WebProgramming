<%@ page contentType="text/html;charset=utf-8" %>
<html>
<head>
   <title>구구단</title></head>
   <SCRIPT LANGUAGE="JavaScript">
   document.write("<center><H1>구구단표</H1></center><br>");
   var first = prompt("첫번째 숫자");
	var second = prompt("두번째 숫자를 입력하세요");
	var result = first * second;
	alert(first+" X "+second+" = "+result);

   document.write("<TABLE border=10 align=center>");
   document.write("<tr><td>1단</td><td>2단</td><td>3단</td><td>4단</td><td>5단</td><td>6단</td><td>7단</td><td>8단</td><td>9단</td></tr>")
   for(i=1;i<10;i++){
	document.write("<tr>")
	for(j=1;j<10;j++){
		document.write("<td>")
		result = i*j;
		document.write(i+" X "+j+" = "+result);
		document.write("</td>")
	}
	document.write("</tr>")
	}
   document.write("</table>");
</SCRIPT>
</head>
<body>
</body>
</html>
